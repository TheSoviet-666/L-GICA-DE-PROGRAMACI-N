def main():
    numeros = []

    print("Introduce 8 números enteros:")

    # Leer 8 números enteros desde el teclado y almacenarlos en el array
    for _ in range(8):
        numero = int(input("Introduce un número entero: "))
        numeros.append(numero)

    # Determinar el número mayor y el número menor
    numero_mayor = max(numeros)
    numero_menor = min(numeros)

    # Mostrar el número mayor y el número menor en la consola
    print("El número mayor es:", numero_mayor)
    print("El número menor es:", numero_menor)

if __name__ == "__main__":
    main()
